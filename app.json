{
  "expo": {
    "name": "기록 책장",
    "slug": "read-book",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "girokshelf",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.gabin.girokshelf",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#FFFAFF",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.gabin.girokshelf"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#fffaff",
          "dark": {
            "image": "./assets/images/splash_dark.png",
            "backgroundColor": "#201d24"
          }
        }
      ],
      "expo-sqlite",
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/MaruBuri-Regular.ttf",
            "./assets/fonts/MaruBuri-SemiBold.ttf",
            "./assets/fonts/Pretendard-Regular.ttf",
            "./assets/fonts/Pretendard-SemiBold.ttf"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "9a91e0f2-e0e1-4dee-98e6-f26d220f2dcd"
      }
    },
    "owner": "sung_gabin",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/9a91e0f2-e0e1-4dee-98e6-f26d220f2dcd"
    }
  }
}
